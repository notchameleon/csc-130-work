package project2;

public abstract class Vehicle {
	protected int hp;
	protected boolean isActive;
	
	public Vehicle(int HP) 
	{
		hp = HP;	
		this.isActive = true;
	}
	
	public int getHp() 
	{
		return hp;
	}
	
	
	public boolean isActive() {
		return isActive;
	}
	
	public void setActive(boolean activity) 
	{
		isActive = activity;
	}
	
	
	public abstract String toString();
	
}
